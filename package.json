{
  "name": "@julusian/jpeg-turbo",
  "version": "2.0.0-0",
  "description": "Limited libjpeg-turbo bindings for Node.js.",
  "keywords": [
    "jpeg",
    "jpeg-turbo",
    "libjpeg-turbo"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/julusian/node-jpeg-turbo.git"
  },
  "binary": {
    "napi_versions": [
      7
    ]
  },
  "engines": {
    "node": ">=14.15"
  },
  "main": "./index.js",
  "types": "./module.d.ts",
  "dependencies": {
    "bindings": "^1.5.0",
    "cmake-js": "github:cmake-js/cmake-js",
    "node-addon-api": "^5.0.0",
    "prebuild-install": "^7.1.0"
  },
  "scripts": {
    "rebuild": "cmake-js compile --target jpegturbo",
    "install": "prebuild-install -r napi || cmake-js compile --target jpegturbo",
    "prebuild": "prebuild -r napi --all --verbose --backend cmake-js -- --target jpegturbo",
    "test": "jest"
  },
  "devDependencies": {
    "jest": "^28.0.3",
    "prebuild": "^11.0.3"
  },
  "resolutions": {
    "prebuild/cmake-js": "github:cmake-js/cmake-js"
  }
}
